{
  "name": "hospital-d-supply-chain",
  "version": "1.0.0",
  "description": "Hospital-D Medical Supply Chain Management System - Team 5",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "seed": "node core/database/seed.js",
    "seed:history": "node core/database/seed-history.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:reset": "node scripts/reset-db.js",
    "docker:up": "docker-compose -f docker/docker-compose.base.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.base.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.base.yml logs -f"
  },
  "keywords": [
    "hospital",
    "supply-chain",
    "medical",
    "inventory",
    "soa",
    "serverless"
  ],
  "author": "Team 5 - Hospital-D",
  "license": "MIT",
  "dependencies": {
    "@azure/event-hubs": "^6.0.2",
    "@azure/identity": "^4.13.0",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "soap": "^1.6.2",
    "winston": "^3.11.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
